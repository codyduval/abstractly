%script{:type => "text/javascript",
        :src  => "http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"}

%script{:type => "text/javascript",
        :src  => "/ab_theme/js/bootstrap.js"}

%script{:type => "text/javascript",
        :src  => "/ab_theme/js/jquery.imagemapster.js"}

%script{:type => "text/javascript",
        :src  => "/ab_theme/js/graph-captions.js"}

%script{:type => "text/javascript",
        :src  => "/ab_theme/js/jquery.easing.js"}

%script{:type => "text/javascript",
        :src  => "/ab_theme/js/chirp.min.js"}

%script{:type => "text/javascript"}
  Chirp({
  user: 'codee',      //Twitter username
  max: 1,           //Maximum number of tweets to show 
  count: 10,          //Total tweets to retrieve
  retweets: true,       //Show/Don't show retweets
  replies: true,       //Show/Don't show replies
  cacheExpire: 1000 * 60 * 60, //Number of milliseconds to cache tweets
  target: 'tweets'
  })

%script{:type => "text/javascript",
        :src  => "/ab_theme/js/jquery.slidorion.js"}

%script{:type => "text/javascript"}
  $(document).ready(function(){
  $('#slidorion').slidorion({
  autoPlay: 'false'
  });
  });

%script{:type => "text/javascript"}
  $(function() {
  $('ul#mynav a').bind('click',function(event){
  var $anchor = $(this);
  $('html, body').stop().animate({
  scrollTop: $($anchor.attr('href')).offset().top + -20
  }, 750,'easeInOutCirc');
  event.preventDefault();
  });
  });